{
  "name": "ts-template",
  "version": "1.0.0",
  "license": "Unlicense",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node build/main",
    "start:dev": "node -r ts-node/register -r tsconfig-paths/register src/main",
    "start:debug": "node --inspect -r ts-node/register -r tsconfig-paths/register src/main",
    "clean": "rimraf build"
  },
  "dependencies": {
    "rimraf": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  }
}
